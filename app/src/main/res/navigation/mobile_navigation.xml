<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.smallandwhitfield.ui.catalogues.CatalogueFragment"
        android:label="@string/title_catalogue"
        tools:layout="@layout/fragment_catalogue" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.smallandwhitfield.ui.interbid.InterbidFragment"
        android:label="@string/title_interbid"
        tools:layout="@layout/fragment_interbid" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.smallandwhitfield.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>